{
    "name": "flaravel/upload",
    "description": "laravel 文件上传",
    "license": "MIT",
    "authors": [
        {
            "name": "fxj",
            "email": "fangxijun0101@meimeik.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Flaravel\\Upload\\": "src/"
        }
    },

    "require": {
        "aliyuncs/oss-sdk-php": "2.3"
    },

    "require-dev": {
        "laravel/laravel": "5.5.*"
